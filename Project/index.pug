	body
		header
			nav.navbar
				.container-fluid
					// Brand and toggle get grouped for better mobile display
					.navbar-header
						button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
							span.sr-only Toggle navigation
							span.icon-bar
							span.icon-bar
							span.icon-bar
					// Collect the nav links, forms, and other content for toggling
					#bs-example-navbar-collapse-1.collapse.navbar-collapse
						ul.nav.navbar-nav
							li.active
								a(href='#') Личная информация 
							li
								a(href='#') Опыт
							li
								a(href='#') JavaScript
							li
								a(href='#') О себе
		.container__inner
			h1 Анкета
			.inform_wrapper
				form(action="" method="post")
					.personal_inform
						h2 Личная информация
						.personal_inform__filling
							.input_style
								input(type="text" id="name" value="Фаррух Фатхуллаев")
								label(for="name") ФИО
							select#birthyear(name='birthyear')
								option(data-display="Год рождения")
								option(value='2000') 2000
								option(value='2006') 1999
								option(value='2005') 1998
								option(value='1997') 1997
								option(value='1996') 1996
								option(value='1995' selected) 1995
								option(value='1994') 1994
								option(value='1993') 1993
								option(value='1992') 1992
								option(value='1991') 1991
								option(value='1990') 1990
								option(value='1989') 1989
								option(value='1988') 1988
								option(value='1987') 1987
								option(value='1986') 1986
							.input_style
								input(type="text" id="addres" value="г.Ташкент")
								label(for="addres") Место проживания
							.input_style
								input(type="text" id="skype" value="Не создан")
								label(for="skype") Skype
							.input_style
								input(type="text" id="e-mail" value="farrux.9522s@gmail.com")
								label(for="e-mail") E-mail
					.skill_inform
						h2 Расскажите о своем опыте
						.row
							.col-xs-12.col-sm-12.col-md-4.col-lg-4
								.skill_inform__checkbox-style
									input(type='checkbox', name='option2', value='a2' id="bem" checked)
									label(for="bem")
										span
										| БЭМ/OOCSS
								.skill_inform__checkbox-style
									input(type='checkbox', name='option3', value='a3' id="less" checked)
									label(for="less")
										span
										| Stylus/LESS/SASS
								.skill_inform__checkbox-style
									input(type='checkbox', name='option4', value='a4' id="svg")
									label(for="svg")
										span
										| Работаю с SVG
							.col-xs-12.col-sm-12.col-md-4.col-lg-4
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="layout" checked)
									label(for="layout")
										span
										| Верстаю семантично
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="es")
									label(for="es")
										span
										| ES2015/ES2016
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="gulp" checked)
									label(for="gulp")
										span
										| Gulp/GRUNT
							.col-xs-12.col-sm-12.col-md-4.col-lg-4
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="webpack")
									label(for="webpack")
										span
										| Webpack
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="jquery" checked)
									label(for="jquery")
										span
										| jQuery
								.skill_inform__checkbox-style
									input(type='checkbox', name='option5', value='a5' id="git" checked)
									label(for="git")
										span
										| Ипользую Git

					.skilljs_inform
						h2 Уровень владения JavaScript
						.slider_wrapper
							.slider(data-color='red')
								#first.slider__body
									.slider__handle.ui-slider-handle
									.slider__scale
										.scale Не владею
										.scale Использую готовые
											br
											|	решения
										.scale Использую готовые решения
											br
											| и умею и переделывать
										.scale Пишу сложный JS с нуля


					.about_myself
						h2 Расскажите о себе
						p Напишите несколько предложений, чем привлекла наша вакансия
							br
							| и что вы ожидаете от работы у нас.
						textarea(placeholder="Меня устраивают условия труда и привлекают перспективы. Уверен, что именно здесь я смогу реализовать свой потенциал" value="")
					input(type="submit" value="Готово")

		script(src="js/jquery.min.js")
